+  mkdir -p ${INSTALL}/etc/X11
+    if find_file_path config/xorg.conf; then
+      cp ${FOUND_PATH} ${INSTALL}/etc/X11
+    fi
+}
+
+post_install() {
+  enable_service xorg.service
+}
